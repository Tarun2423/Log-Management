<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
 
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/llogin</url-pattern>
    </servlet-mapping>
<servlet>
        <servlet-name>LogReceiver</servlet-name>
        <servlet-class>LogReceiver</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogReceiver</servlet-name>
        <url-pattern>/sendlog</url-pattern>
    </servlet-mapping>
<servlet>
        <servlet-name>GetLogs</servlet-name>
        <servlet-class>GetLogs</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetLogs</servlet-name>
        <url-pattern>/getlogs</url-pattern>
    </servlet-mapping>
 <servlet>
        <servlet-name>GetTomcatlogs</servlet-name>
        <servlet-class>GetTomcatlogs</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetTomcatlogs</servlet-name>
        <url-pattern>/getTomcatlogs</url-pattern>
    </servlet-mapping>
  <servlet>
        <servlet-name>SendEmail</servlet-name>
        <servlet-class>SendEmail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SendEmail</servlet-name>
        <url-pattern>/sendEmail</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>JNIlogs</servlet-name>
        <servlet-class>JNIlogs</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JNIlogs</servlet-name>
        <url-pattern>/jni</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateJNI</servlet-name>
        <servlet-class>UpdateJNI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateJNI</servlet-name>
        <url-pattern>/upjni</url-pattern>
    </servlet-mapping>
      <servlet>
        <servlet-name>Addnotification</servlet-name>
        <servlet-class>Addnotification</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Addnotification</servlet-name>
        <url-pattern>/addnot</url-pattern>
    </servlet-mapping>
      <servlet>
        <servlet-name>GetNotification</servlet-name>
        <servlet-class>GetNotification</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetNotification</servlet-name>
        <url-pattern>/notget</url-pattern>
    </servlet-mapping>
<welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>

</web-app>
